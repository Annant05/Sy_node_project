<!DOCTYPE html>
<html lang="en">

<!--suppress HtmlRequiredTitleElement -->
<head>
    <% include partials/head_css.ejs %>
    <style>
        table, th, td {
            border: 1px solid rgba(156, 156, 156, 0.83);
            border-spacing: 2px;
            padding: 4px;
        }
    </style>
</head>

<body class="animsition">
<div class="page-wrapper">

    <% include partials/header_mobile.ejs %>
    <% include partials/menu_sidebar.ejs %>

    <!-- PAGE CONTAINER-->
    <div class="page-container">
        <% include partials/header_desktop.ejs %>

        <!-- MAIN CONTENT-->
        <div class="main-content">
            <div class="section__content section__content--p30">
                <div class="container-fluid">
                    <div class="col-lg-12 col-md-12">
                        <div class="row">
                            <table>
                                <tr>
                                    <th>Resource Name</th>
                                    <th>Resource Value</th>
                                </tr>
                                <% if(outdata !== null ){
                                outdata.forEach( function(output){ %>
                                    <tr>
                                        <td> <%= output['OutputKey'] %></td>
                                        <td><%= output['OutputValue'] %></td>
                                    </tr>
                                <% });}
                                else { let str = ' Stack is still being created. Please wait for some time and refresh. ' %>
                               <p> <%=str%></p>
                                <% } %>
                            </table>
                        </div>
                    </div>

                    <!--- Action Second Section End (More Functionality can be added here, if we extend this project)---->

                    <div class="row">
                        <div class="col-md-12">
                            <div class="copyright">
                                <p>Developed by Team Clustroit | TCS Inframind Contest</p>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- END MAIN CONTENT-->
        <!-- END PAGE CONTAINER-->
    </div>

</div>

<% include partials/bottom_js_scripts.ejs %>

<!-- Main JS-->
<script src="/js/main.js"></script>

</body>
</html>
<!-- end document-->
